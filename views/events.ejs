header{
    z-index: 999;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: rgba(103, 18, 18, 0.953);
    box-shadow: 0 5px 25px rgb(0 0 0 / 20%);
    padding: 0 100px;
    transition: 0.6s;
}
 .logo {
color: aliceblue;
font-family: 'Raleway', sans-serif;
font-size: 1.8em;
font-weight: 400;
text-transform: uppercase;
text-decoration: none;
letter-spacing: 2px;

}
 .navigation{
  position: relative;
  line-height: 75px; 
  transition-property: line-height;
}
header.sticky .navigation{
    line-height: 60px;
}
 .navigation .menu{
    position: relative;
    display: flex;
    justify-content: center;
    list-style: none;
    user-select:none;
}
.menu_item > a{
   color: aliceblue;
   font-size: 1em;
   text-decoration: none;
   margin: 20px;
   padding: 25px 0;
}
.menu_item>a:hover{
  color: black;
  transition: 0.3s;
}
.menu_item .sub-menu{
    position: absolute;
    background: rgb(100, 20, 20);
    top:74px;
   line-height: 40px;
   list-style: none;
   border-radius:  0 0 8px 8px;
   box-shadow: 0 5px 25px rgba(192, 26, 26, 0.2);
   pointer-events: none;
   transform: translateY(20px);
   opacity: 0;
   transition: 0.3s;
   transition-property: transform,opacity;
}
span {
    color:black;
}

header.sticky .menu_item .sub-menu{
    top: 60px;
}
.menu_item:hover .sub-menu{
    pointer-events: all;
    transform: translateY(0);
    opacity: 1; 
    
}

.menu_item .sub-menu .sub-item{
    position: relative;
    padding: 7px 0;
    cursor: pointer;
    box-shadow: inset 0px -30px 5px -30px rgba(26, 12, 12, 0.251);

}

.menu_item .sub-menu .sub-item a{
    color: #fff;
    font-size: 1em;
    text-decoration: none;
    padding: 15px 30px;
  
}
.menu_item .sub-menu .sub-item:hover{
    background: black;
    
}
.menu_item .sub-menu .sub-item:last-child:hover{
    border-radius:0 0 8px 8px;
}

.menu-btn{
    display: none;
}
@media (max-width:1060px) {
    header .navigation .menu{
        position: fixed;
        display: block;
        background: rgb(114, 41, 41);
        min-width: 350px;
        height: 100vh;
        top: 0;
        right: -100%;
        padding: 90px 50px;
        overflow-y: auto;
        transition: 0.5s;
        transition-property: right,visiblilty;
        
    }
    header.sticky .navigation{
        line-height: 75px;
    }
    header .navigation .menu.active{
        right: 0; 
        visibility: visible;
    }
    .menu_item{
        position: relative;
    }
    .menu_item .sub-menu{
        opacity: 1;
        position: relative;
        top: 0;
        transform: translateX(10px);
        background: rgba(192, 26, 26, 0.2);
        border-radius: 5px;
        overflow: hidden;
        display: none;
    }
    header.sticky .menu_item .sub-menu{
        top: 0;
    }
    .menu_item:hover .sub-menu{
        transform: translateX(10px); 
    } 
    .menu_item .sub-menu .sub-item{
        box-shadow: none;
    }
    .menu_item .sub-menu .sub-item:hover{
background: none;
    }
    .menu_item .sub-menu .sub-item a:hover{
        color: black;
        transition: 0.3s;
    }

